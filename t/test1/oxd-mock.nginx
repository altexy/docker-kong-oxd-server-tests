worker_processes 1; # one is importnt here! we load a model by http request

events {
    worker_connections 512;
}

error_log logs/error.log debug;

http {
    server {
        listen 80;

        location / {
            content_by_lua_block {
                require"gluu.oxd-mock"(require"gluu.oxd-model")
            }
        }
    }
}
